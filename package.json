{
  "name": "app",
  "version": "1.0.0",
  "scripts": {
    "dev:build-server": "webpack --config webpack.server.js --mode=development -w",
    "dev:build-client": "webpack --config webpack.config.js --mode=development -w",
    "build-server": "webpack --config webpack.server.js --mode=production",
    "build-client": "webpack --config webpack.config.js --mode=production",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "build:start": "cd dist && PORT=3000 npx serve",
    "start": "node ./server-build/index.js",
    "start:live": "webpack serve --open --mode development --live-reload --hot"
  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "@babel/preset-typescript": "^7.10.4",
    "@types/express": "^4.17.14",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.2",
    "autoprefixer": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^6.3.0",
    "html-webpack-plugin": "^5.3.2",
    "nodemon": "^2.0.20",
    "postcss": "^8.2.1",
    "postcss-loader": "^4.1.0",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.5.2",
    "webpack": "^5.57.1",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "@types/cors": "^2.8.12",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "webpack-node-externals": "^3.0.0"
  }
}
